package segunda.aep;

public class AppShow {

	public static void main(String[] args) {
		try {
			Show ButecoDoGusttavoLima = new Show("Gusttavo Lima ", 18, "Bar do Zé", 70, 80);

			Show JorgeeMateus = new Show("Jorge ", 18, "Bar do Zé", 90, 80);

			System.out.println(ButecoDoGusttavoLima);

			System.out.println(JorgeeMateus);

		} catch (RuntimeException e) {
			System.out.println("Mensagem de exceção: " + e.getMessage());
		}

	}

}
